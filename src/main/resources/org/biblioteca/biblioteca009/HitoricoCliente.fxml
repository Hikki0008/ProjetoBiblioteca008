<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableCell?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Scene?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.biblioteca.biblioteca009.Controllers.HistoricoController">
    <Label text="Histórico do Cliente" font="Arial 20"/>
    <TableView fx:id="tableViewHistorico">
        <TableColumn fx:id="colData" text="Data" prefWidth="150"/>
        <TableColumn fx:id="colTipo" text="Tipo" prefWidth="100"/>
        <TableColumn fx:id="colDescricao" text="Descrição" prefWidth="300"/>
    </TableView>
    <HBox spacing="10">
        <Button text="Voltar para Início" onAction="#onVoltarInicio"/>
    </HBox>

</VBox>
